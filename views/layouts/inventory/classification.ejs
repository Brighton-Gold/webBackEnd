<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/style.css">
    <title><%= title %></title>

</head>

<body>
  <nav>
    <%= nav %>
  </nav>

<main>
  <% if (title) { %>
    <h1><%= title %></h1>
    <% } else {
      res.redirect('/')
    } %>

<%# messages() %>

<%- grid %>

<div class = 'cars'>
  <% cars.forEach(function(car) { %>
    <div class="car">
      <h2><%= car.inv_make %></h2>
      <% }); %>
</div>

</main>
</body>
</html>